<%- include("partials/header") -%>
        <div class="wrapper wrapper--w790">
            <div class="card card-5">
                <div class="card-heading">
                    <h1 class="title">DREAM HOME </h1>
                    <h2 class="title">property registration form</h2>
                </div>
                <div class="card-body">
                    <form method="POST" action="/property">
                        <div>
                            <div class="form-row m-b-55">
                                <div class="name">property id</div>
                                <div class="value">
                                    <div class="row row-space">
                                        <div class="col-2">
                                            <div class="input-group-desc">
                                                <input class="input--style-5" type="text" name="propertyNo">
                                               
                                            </div>
                                        </div>
                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="form-row m-b-55">
                            <div class="name">type</div>
                            <div class="value">
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group-desc">
                                            <input class="input--style-5" type="text" name="type">
                                         
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                            
                        </div>

                        <div class="form-row m-b-55">
                            <div class="name">rooms</div>
                            <div class="value">
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group-desc">
                                            <input class="input--style-5" type="number" name="rooms">
                                         
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                            
                        </div>
                        <div class="form-row m-b-55">
                            <div class="name">rent</div>
                            <div class="value">
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group-desc">
                                            <input class="input--style-5" type="number" name="rent">
                                         
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                            
                        </div>


                        <div class="form-row m-b-55">
                            <div class="name">street</div>
                            <div class="value">
                                <div class="row row-space">
                                    <div class="col-2">
                                        <div class="input-group-desc">
                                            <input class="input--style-5" type="text" name="street">
                                        
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>

                      


<div class="form-row m-b-55">
    <div class="name">city</div>
    <div class="value">
        <div class="row row-space">
            <div class="col-2">
                <div class="input-group-desc">
                    <input class="input--style-5" type="text" name="city">
                
                </div>
            </div>
            
        </div>
    </div>
</div>

<div class="form-row m-b-55">
    <div class="name">postal code</div>
    <div class="value">
        <div class="row row-space">
            <div class="col-2">
                <div class="input-group-desc">
                    <input class="input--style-5" type="text" name="postcode">
                
                </div>
            </div>
            
        </div>
    </div>
</div>
    <hr>                   


                        <div class="form-row" id="position">
                            <div class="name" >owner id</div>
                            <div class="value">
                                <div class="input-group">
                                    <input class="input--style-5" type="text" name="ownerNo" >
                                  
                                </div>
                            </div>
                        </div>
                        <hr>            

<h3 id="position">Enter details if applicable</h3>
                        <hr>
                        <div class="form-row m-b-55" id="position">
                            
                                <div class="name">managed by<br> staff</div>
                                <div class="value">
                                <div class="row row-refine">
                                    <div class="col-5">
                                        <div class="input-group-desc">
                                            <input class="input--style-5" type="text" name="staffNo">
                                           
                                        </div>
                                    </div>
                                    
                                </div>
                            
                            </div>
                            
                        </div>       
                        <div>
                            <button class="btn btn--radius-2 btn--red" type="submit">Register</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>

<%- include("partials/footer") -%>